class Circle
{
    Radius:number;
    PI:number=3.14;

    constructor(radius:number)
    {
        this.Radius=radius;
    }

Area()
{
    var Ans:number=0;
    Ans=3.14*this.Radius*this.Radius;
    return Ans;
}

}

var object1=new Circle(4);
var object2 = new Circle(7);

var Ret:number=0;
Ret= object1.Area()
console.log("Area of Circle is:"+Ret);

Ret=object2.Area()
console.log("Area of Circle is:"+Ret);
